<h1>Listing Users</h1>

<table>
  <tr>
    <th>E-mail</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', profile_path(user) %></td>
    <td><%= link_to 'Edit', edit_profile_path(user) %></td>
    <td><%= link_to 'Destroy', profiles_path(user), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

